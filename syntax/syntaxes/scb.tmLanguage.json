{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "vscode-scb",
	"scopeName": "text.scb",
	"patterns": [
		{
			"include": "#indent1"
		},
		{
			"include": "#indent2"
		},
		{
			"include": "#link"
		}
	],
	"repository": {
		"indent1": {
			"patterns": [{
				"match": "^( )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.1.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent2": {
			"patterns": [{
				"match": "^(  )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.2.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent3": {
			"patterns": [{
				"match": "^(   )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.3.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent4": {
			"patterns": [{
				"match": "^(    )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.4.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent5": {
			"patterns": [{
				"match": "^(     )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.5.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent6": {
			"patterns": [{
				"match": "^(      )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.6.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent7": {
			"patterns": [{
				"match": "^(       )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.7.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent8": {
			"patterns": [{
				"match": "^(        )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.8.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent9": {
			"patterns": [{
				"match": "^([ ]{8}[ ]+)([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.over9.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"link": {
			"patterns": [{
				"name": "link.scb",
				"match": "\\[[^\\]]+\\]"
			}]
		}
	}
}