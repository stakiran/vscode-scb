{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "vscode-scb",
	"scopeName": "text.scb",
	"patterns": [
		{
			"include": "#indent1"
		},
		{
			"include": "#indent2"
		},
		{
			"include": "#link"
		}
	],
	"repository": {
		"indent1": {
			"patterns": [{
				"name": "indent.1.scb",
				"match": "^( )[^ ]"
			}]
		},
		"indent2": {
			"patterns": [{
				"name": "indent.2.scb",
				"match": "^(  )[^ ]"
			}]
		},
		"link": {
			"patterns": [{
				"name": "link.scb",
				"match": "\\[[^\\]]+\\]"
			}]
		}
	}
}