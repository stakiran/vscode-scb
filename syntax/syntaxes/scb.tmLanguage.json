{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "vscode-scb",
	"scopeName": "text.scb",
	"patterns": [
		{
			"include": "#indent1"
		},
		{
			"include": "#indent2"
		},
		{
			"include": "#link"
		}
	],
	"repository": {
		"indent1": {
			"patterns": [{
				"match": "^( )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.1.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent2": {
			"patterns": [{
				"match": "^(  )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.2.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"link": {
			"patterns": [{
				"name": "link.scb",
				"match": "\\[[^\\]]+\\]"
			}]
		}
	}
}