{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "vscode-scb",
	"scopeName": "text.scb",
	"patterns": [
		{
			"include": "#indent1"
		},
		{
			"include": "#indent2"
		},
		{
			"include": "#indent3"
		},
		{
			"include": "#indent4"
		},
		{
			"include": "#indent5"
		},
		{
			"include": "#indent6"
		},
		{
			"include": "#indent7"
		},
		{
			"include": "#indent8"
		},
		{
			"include": "#indent9over"
		},
		{
			"include": "#link"
		},
		{
			"include": "#quote-statement"
		},
		{
			"include": "#quote"
		},
		{
			"include": "#literal"
		},
		{
			"include": "#codeblock-statememt"
		},
		{
			"include": "#codeblock"
		}
	],
	"repository": {
		"indent1": {
			"patterns": [{
				"match": "^( )(?=[^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.1.scb"
					}
				}
			}]
		},
		"indent2": {
			"patterns": [{
				"match": "^(  )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.2.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent3": {
			"patterns": [{
				"match": "^(   )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.3.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent4": {
			"patterns": [{
				"match": "^(    )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.4.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent5": {
			"patterns": [{
				"match": "^(     )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.5.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent6": {
			"patterns": [{
				"match": "^(      )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.6.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent7": {
			"patterns": [{
				"match": "^(       )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.7.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent8": {
			"patterns": [{
				"match": "^(        )([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.8.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"indent9over": {
			"patterns": [{
				"match": "^([ ]{8}[ ]+)([^ ])",
				"captures" : {
					"1" : {
						"name" : "indent.over9.scb"
					},
					"2" : {
						"name" : "nohilight.scb"
					}
				}
			}]
		},
		"link": {
			"patterns": [{
				"name": "link.scb",
				"match": "\\[[^\\]]+\\]"
			}]
		},
		"quote-statement": {
			"begin": "^(?<=[ ]*)>",
			"end": "$",
			"patterns": [
				{
					"include": "#quote"
				}
			]
		},
		"quote": {
			"begin": ">.+",
			"end": "$",
			"beginCaptures": {
				"0": { "name": "quote.line.start.scb" }
			},
			"endCaptures": {
				"0": { "name": "quote.line.end.scb" }
			},
			"name": "quote.line.scb"
		},
		"literal": {
			"patterns": [{
				"name": "literal.scb",
				"match": "`[^`]+`"
			}]
		},
		"codeblock-statement": {
			"begin": "^(?<=[ ]*)code\\:",
			"end": "^(?<=[ ]*)\\:c",
			"patterns": [
				{
					"include": "#codeblock"
				}
			]
		},
		"codeblock": {
			"begin": "code\\:.+",
			"end": "\\:c",
			"beginCaptures": {
				"0": { "name": "codeblock.start.scb" }
			},
			"endCaptures": {
				"0": { "name": "codeblock.end.scb" }
			},
			"name": "codeblock.scb"
		}
	}
}